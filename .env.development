# Development environment for testing without real API keys
NODE_ENV=development
SKIP_CONFIG_VALIDATION=true

# Placeholder API keys for development testing
REDDIT_CLIENT_ID=dev_reddit_client_id
REDDIT_CLIENT_SECRET=dev_reddit_client_secret
REDDIT_USER_AGENT=CPTO/1.0 by DevUser
REDDIT_USERNAME=dev_username
REDDIT_PASSWORD=dev_password

# Placeholder OpenAI key
OPENAI_API_KEY=dev_openai_key
OPENAI_MODEL=gpt-4-turbo

# Placeholder TokenMetrics key
TOKENMETRICS_API_KEY=dev_tokenmetrics_key
TOKENMETRICS_BASE_URL=https://api.tokenmetrics.com

# Development configuration
PORT=3000
LOG_LEVEL=debug

# Trading configuration - safe for development
SUBREDDITS=CryptoCurrency,Bitcoin
SENTIMENT_THRESHOLD=0.8
TRADE_AMOUNT_USD=1
MAX_TRADES_PER_HOUR=1

# Database
DATABASE_PATH=./data/cpto-dev.db